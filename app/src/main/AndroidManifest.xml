<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.joni.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Pistas"
            android:label="@string/Musica"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".ReproduccionActual"
            android:label="@string/Reproduciendo" />
        <activity
            android:name=".ListaReproduccion"
            android:label="@string/ListaReproduccion" />
        <activity
            android:name=".Ecualizer"
            android:label="@string/Ecualizador"></activity>
    </application>

</manifest>